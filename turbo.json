{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "./build/**",
        "./.pkg/**",
        "./dist/**",
        "./node/**",
        "./react/**"
      ]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "watch": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
